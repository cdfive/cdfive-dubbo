<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://dubbo.apache.org/schema/dubbo
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

	<!--<dubbo:application name="cdfive-mp3-service" />-->
	<dubbo:application name="cdfive-mp3-service" logger="slf4j" />

	<dubbo:registry protocol="zookeeper" address="zookeeper://localhost:2181" />

	<dubbo:protocol name="dubbo" port="${dubbo.port}" dispatcher="all" threadpool="fixed" threads="200" />

	<dubbo:service ref="songService" interface="com.cdfive.mp3.service.SongService" />
	<dubbo:service ref="categoryService" interface="com.cdfive.mp3.service.CategoryService" />

	<dubbo:provider timeout="5000" retries="0" filter="-exception"/>

	<dubbo:consumer check="false" />

</beans>
